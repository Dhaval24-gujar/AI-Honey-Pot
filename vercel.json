{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/honeypot",
            "dest": "main.py"
        },
        {
            "src": "/health",
            "dest": "main.py"
        },
        {
            "src": "/sessions/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "GROQ_API_KEY": "@groq_api_key",
        "API_KEY": "@api_key",
        "GUVI_ENDPOINT": "@guvi_endpoint"
    }
}